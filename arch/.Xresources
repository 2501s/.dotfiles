st.font:SourceCodePro-Light.otf:size=12:style=Light:antialias=true,xft:Baekmuk Gulim: size=13

!hard contrast: *background: #1d2021
*background: #262626
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

! Extensions

URxvt.perl-ext-common: matcher,keyboard-select
! Keyboard select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
! Url matcher
URxvt.matcer.button: 1
URxvt.url-launcher: /usr/bin/xdg-open

! Fikser ibus-funksjonalitet i urxvt
! Start-kommandoen for ibus er satt i .xinitrc ibus -xim
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot

